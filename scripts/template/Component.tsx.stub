import styled from 'styled-components'
import { defaultStyledProps, globalFont, StyledProps } from '../utils/styled'

interface {Component}Props extends StyledProps {
  someFlag?: boolean
}

export const {Component} = styled.div<{Component}Props>`
  ${globalFont};

  color: ${props => props.someFlag ? 'red' : 'green'};
`

{Component}.defaultProps = {
  ...defaultStyledProps
}
